# 全角→半角（数字・小数点・マイナスのみ）
ZEN2HAN = str.maketrans("０１２３４５６７８９．－", "0123456789.-")

def normalize_zenkaku_digits(s: pd.Series) -> pd.Series:
    if s.dtype != object:
        return s
    return s.str.translate(ZEN2HAN)

